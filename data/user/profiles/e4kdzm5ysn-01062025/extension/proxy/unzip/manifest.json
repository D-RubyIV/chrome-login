{
      "name": "Chrome Proxy",
      "version": "1.0.0",
      "manifest_version": 3,
      "permissions": [
        "proxy",
        "storage",
        "webRequest",      
        "webRequestAuthProvider"
      ],
      "host_permissions": [
        "<all_urls>"
      ],
      "background": {
        "service_worker": "background.js"
      },
      "minimum_chrome_version": "108"
    }